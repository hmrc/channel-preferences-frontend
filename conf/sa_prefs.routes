GET     /print-preferences/:token                            @controllers.sa.prefs.external.SaPrefsController.index(token: String, return_url: String, emailAddress: Option[controllers.sa.prefs.EncryptedEmail])
GET     /print-preferences-saved                             @controllers.sa.prefs.external.SaPrefsController.confirm(token: String, return_url: String)
GET     /print-preferences-no-action                         @controllers.sa.prefs.external.SaPrefsController.noAction(return_url: String, digital: Boolean)
POST    /print-preferences/:token                            @controllers.sa.prefs.external.SaPrefsController.submitPrefsForm(token: String, return_url: String)

GET     /print-preferences/verification/:token               @controllers.sa.prefs.external.EmailValidationController.verify(token: String)

# Serves up the test gif that is used to determine if the service is up
GET     /print-preferences/assets/*file                      controllers.sa.prefs.Assets.at(path="/public", file)
