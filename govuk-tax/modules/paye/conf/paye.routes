GET         /home                                                         @controllers.paye.PayeHomeController.home

GET         /benefits                                                     @controllers.paye.BenefitHomeController.listBenefits

GET         /benefits/:kind/:year/:employmentSequenceNumber/remove        @controllers.paye.RemoveBenefitController.benefitRemovalForm(kind: Int, year: Int, employmentSequenceNumber: Int)
POST        /benefits/:kind/:year/:employmentSequenceNumber/remove        @controllers.paye.RemoveBenefitController.requestBenefitRemoval(kind: Int, year: Int, employmentSequenceNumber: Int)
POST        /benefits/:kinds/:year/:employmentSequenceNumber/confirm       @controllers.paye.RemoveBenefitController.confirmBenefitRemoval(kinds: String, year: Int, employmentSequenceNumber: Int)
GET         /benefits/:kind/confirmation/:oid                             @controllers.paye.RemoveBenefitController.benefitRemoved(kind: Int, oid: String)

